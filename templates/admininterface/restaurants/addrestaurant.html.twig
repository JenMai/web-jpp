{% extends 'baseform.html.twig'%}

{% block title %}
Ajouter restaurant
{% endblock %}

{% block h1 %}Ajouter restaurant{% endblock %}

{% block form %}
    {{ form_start(form) }}
        {{ form_errors(form) }}
        <div>
            {{ form_label(form.libelle) }}
            {{ form_errors(form.libelle) }}
            {{ form_widget(form.libelle) }}
        </div>

        <div>
            {{ form_label(form.type) }}
            {{ form_errors(form.type) }}
            {{ form_widget(form.type) }}
        </div>

        <div>
            {{ form_label(form.vege) }}
            {{ form_errors(form.vege) }}
            {{ form_widget(form.vege) }}
        </div>

        <div>
            {{ form_label(form.image) }}
            {{ form_errors(form.image) }}
            {{ form_widget(form.image) }}
        </div>

        <div>
            {{ form_label(form.description) }}
            {{ form_errors(form.description) }}
            {{ form_widget(form.description) }}
        </div>
        <button type="submit" class="btn btn-primary btn-md">Ajouter</button>
    {{ form_end(form) }}
{% endblock %}
