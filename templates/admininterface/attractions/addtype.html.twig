{% extends 'baseform.html.twig'%}

{% block title %}
Ajouter type d'hôtel
{% endblock %}

{% block h1 %} Ajouter un type d'hôtel {% endblock %}

{% block form %}
{{ form_start(form) }}
    {{ form_errors(form) }}
    <div>
        {{ form_label(form.libelle) }}
        {{ form_errors(form.libelle) }}
        {{ form_widget(form.libelle) }}
    </div>
    <button type="submit" class="btn btn-primary btn-md">Ajouter</button>
{{ form_end(form) }}

{% endblock %}
